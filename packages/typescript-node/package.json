{
  "name": "@carel-coenraad/js-boilerplate",
  "version": "0.0.1",
  "description": "JavaScript Boilerplate",
  "private": true,
  "license": "MIT",
  "scripts": {
    "format": "npm run lint:script -- --fix && npm run format:prettier",
    "format:prettier": "prettier \"./**/*.{css,html,js,json,jsx,md,ts,tsx,yaml,yml}\" --write --ignore-path .gitignore",
    "hook:commit-msg": "commitlint --edit \"$1\"",
    "hook:pre-commit": "pretty-quick --staged",
    "lint": "npm run lint:md && npm run lint:script",
    "lint:md": "markdownlint *.md",
    "lint:script": "eslint \"./**/*.{js,jsx,ts,tsx}\" --ignore-path .gitignore",
    "prepare": "husky install",
    "test": "npm run lint"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "markdownlint-cli": "^0.28.1",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
